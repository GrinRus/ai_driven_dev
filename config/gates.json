{
  "feature_slug_source": "docs/.active_feature",
  "api_contract": false,
  "db_migration": false,
  "tests_required": "disabled",
  "deps_allowlist": false,
  "prd_review": {
    "enabled": true,
    "branches": [
      "feature/*",
      "release/*",
      "hotfix/*"
    ],
    "skip_branches": [
      "docs/*"
    ],
    "approved_statuses": [
      "approved"
    ],
    "blocking_statuses": [
      "blocked"
    ],
    "allow_missing_section": false,
    "require_action_items_closed": true
  },
  "qa": {
    "enabled": true,
    "branches": [
      "feature/*",
      "release/*",
      "hotfix/*"
    ],
    "skip_branches": [
      "docs/*"
    ],
    "requires": [
      "gate-tests",
      "gate-api-contract"
    ],
    "command": [
      "python3",
      "scripts/qa-agent.py"
    ],
    "timeout": 600,
    "report": "reports/qa/{slug}.json",
    "block_on": [
      "blocker",
      "critical"
    ],
    "warn_on": [
      "major",
      "minor"
    ],
    "allow_missing_report": false
  }
}
